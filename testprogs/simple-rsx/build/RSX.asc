10REM > rsx loader
20H=1+HIMEM-&0030:MEMORY H-1
30LOAD"rsx.bin",H
40WHILE 1:READ A
50IF A=-1 THEN CALL H:END
60IF A=>&8000 THEN GOSUB 100 ELSE GOSUB 200
70WEND
80:
100A=A-&8000:READ B:IF B>=256THEN POKE H+A,H+PEEK(H+A) ELSE POKE H+A,((H+B)/256)+PEEK(H+A)
110RETURN
120:
200B=H+PEEK(H+A)+256*PEEK(H+A+1):POKE H+A,B:POKE H+A+1,B/256
210RETURN
220:
1000 DATA &0001,&0004,&0007,&000B,&000E,&0011,&0014,&0017,&801A,&100,&801C,&10,&001E,&002A,&802C,&10,&802D,&100,&802F,&100,&802F,&00,-1
